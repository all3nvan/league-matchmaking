<h3 class="text-center">Select Players</h3>
<div class="container">
	<%= form_tag matchmaking_index_path, method: :post do %>
		<% @players.each do |player| %>
			<div class="checkbox"><%= check_box_tag player.id, 'selected' %><%= player.name %></div>
		<% end %>
		<%= submit_tag "Create Match", class: "btn btn-success" %>
	<% end %>
</div>
<h3 class="text-center"><%= @player.name %></h3>
<div class="container">
	<table class="table table-striped">
		<thead>
			<tr>
				<th>Rank</th>
				<th>Champion</th>
				<th>Winrate</th>
				<th>Wins</th>
				<th>Losses</th>
			</tr>
		</thead>
		<tbody>
			<% @player.champion_stats.each.with_index do |champion, i| %>
				<tr>
					<td><%= i + 1%></td>
					<td><%= champion[:id] %></td>
					<td><%= champion[:winrate] %></td>
					<td><%= champion[:wins] %></td>
					<td><%= champion[:losses] %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>
